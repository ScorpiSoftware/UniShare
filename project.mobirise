        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".content-wrapper": {
              ".logo-wrapper": {
                "margin-top": "80px",
                "margin-bottom": "36px",
                "img": {
                  "display": "inline-flex",
                  "width": "118px",
                  "height": "118px",
                  "object-fit": "contain",
                  "border-radius": "0 !important"
                }
              },
              ".mbr-section-title": {
                "margin-bottom": "60px",
                "@media (max-width: 992px)": {
                  "margin-bottom": "48px"
                }
              },
              ".text-wrapper": {
                "margin-top": "100px",
                "@media (max-width: 992px)": {
                  "margin-top": "48px"
                },
                ".mbr-text": {
                  "display": "inline-block",
                  "width": "50%",
                  "margin-bottom": "0",
                  "@media (max-width: 992px)": {
                    "width": "100%"
                  },
                  "span": {
                    "background": "linear-gradient(90deg, @grad, lighten(@grad, 10%) 65%, lighten(@grad, 20%))",
                    "-webkit-background-clip": "text",
                    "-webkit-text-fill-color": "transparent"
                  }
                }
              },
              ".mbr-section-btn": {
                "margin-top": "36px"
              }
            },
            ".slider-wrapper": {
              "display": "flex",
              "justify-content": "center"
            },
            ".embla__slide": {
              "display": "flex",
              "justify-content": "center",
              "position": "relative",
              "min-width": "100%",
              "max-width": "100%",
              "@media (max-width: 767px)": {
                "min-width": "100%",
                "max-width": "100%"
              },
              "&:hover, &:focus": {
                ".slide-content": {
                  "border": "2px solid @active",
                  "background-color": "@active"
                }
              },
              ".slide-content": {
                "width": "100%",
                "border": "2px solid transparent",
                "transition": "all 0.3s ease-in-out",
                ".item-wrapper": {
                  "padding": "12px",
                  "background-color": "@card",
                  "border": "1px solid @border",
                  "display": "flex",
                  "height": "100%",
                  "justify-content": "space-between",
                  "@media (max-width: 992px)": {
                    "display": "block",
                    "padding": "16px"
                  },
                  ".item-img": {
                    "width": "34%",
                    "margin-right": "16px",
                    "@media (max-width: 992px)": {
                      "width": "100%",
                      "margin-right": "0",
                      "margin-bottom": "16px"
                    },
                    "img": {
                      "height": "115px",
                      "width": "100%",
                      "object-fit": "cover",
                      "@media (max-width: 992px)": {
                        "height": "180px"
                      }
                    },
                    "a": {
                      "display": "block",
                      "width": "100%",
                      "height": "100%"
                    }
                  },
                  ".item-content": {
                    "width": "66%",
                    "position": "relative",
                    "z-index": "1",
                    "display": "flex",
                    "justify-content": "flex-end",
                    "@media (max-width: 992px)": {
                      "width": "100%",
                      "display": "block"
                    },
                    ".text-wrap": {
                      "display": "flex",
                      "flex-direction": "column",
                      "justify-content": "center",
                      "margin-right": "16px",
                      "width": "100%",
                      "@media (max-width: 992px)": {
                        "width": "100%",
                        "margin-right": "0",
                        "margin-bottom": "16px"
                      },
                      ".item-title": {
                        "margin-bottom": "4px"
                      },
                      ".item-text": {
                        "margin-bottom": "0"
                      }
                    },
                    ".icon-wrapper": {
                      "min-width": "24px",
                      "display": "flex",
                      "align-items": "flex-end",
                      "height": "100%",
                      "@media (max-width: 992px)": {
                        "min-width": "auto"
                      },
                      ".mbr-iconfont": {
                        "font-size": "24px",
                        "color": "@icon"
                      }
                    }
                  }
                }
              }
            },
            ".embla__button--next, .embla__button--prev": {
              "display": "flex"
            },
            ".embla__button": {
              "bottom": "0",
              "width": "48px",
              "height": "48px",
              "font-size": "24px",
              "background-color": "@arrowsBg !important",
              "color": "@arrowsColor !important",
              "border": "none !important",
              "border-radius": "50%",
              "transition": "all 0.3s",
              "position": "absolute",
              "display": "flex",
              "justify-content": "center",
              "align-items": "center",
              "opacity": "1 !important",
              "& when not (@arrows)": {
                "display": "none"
              },
              "& when not (@arrowsMobile)": {
                "@media (max-width: 768px)": {
                  "display": "none"
                }
              },
              "&:hover": {
                "transform": "scale(1.05)"
              },
              "&.embla__button--prev": {
                "left": "0"
              },
              "&.embla__button--next": {
                "right": "0"
              }
            },
            ".embla": {
              "position": "relative",
              "width": "50%",
              "@media (max-width: 992px)": {
                "width": "100%"
              },
              "& when (@arrows)": {
                "padding-bottom": "4rem"
              }
            },
            ".embla__viewport": {
              "overflow": "hidden",
              "width": "100%",
              "margin-right": "1rem"
            },
            ".embla__viewport.is-draggable": {
              "cursor": "grab"
            },
            ".embla__viewport.is-dragging": {
              "cursor": "grabbing"
            },
            ".embla__container": {
              "display": "flex",
              "user-select": "none",
              "-webkit-touch-callout": "none",
              "-khtml-user-select": "none",
              "-webkit-tap-highlight-color": "transparent"
            },
            ".item-menu-overlay": {
              "opacity": "0 !important"
            },
            ".mbr-section-title": {
              "color": "#ffffff"
            },
            ".mbr-text, .text-wrapper": {
              "color": "#ffffff",
              "text-align": "center"
            },
            ".item-title": {
              "color": "#9aa0a6"
            },
            ".item-text": {
              "color": "#ffffff"
            },
            ".mbr-section-title, .mbr-section-btn, .logo-wrapper": {
              "text-align": "center"
            }
          },
          "_name": "header01",
          "_sourceTheme": "progressm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"header01 mbr-embla progressm5\" group=\"Header\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\" checked>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\">\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Title\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"color\" title=\"Gradient Color\" value=\"#3b6bff\" name=\"grad\" condition=\"showText\" selected>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked>\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked>\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\">\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked>\n        <input type=\"color\" name=\"arrowsBg\" title=\"Arrows BG\" value=\"#252525\" condition=\"arrows\">\n        <input type=\"color\" name=\"arrowsColor\" title=\"Arrows Color\" value=\"#ffffff\" condition=\"arrows\">\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" name=\"showCardTitle\" title=\"Card Title\" checked>\n        <input type=\"checkbox\" name=\"showCardText\" title=\"Card Text\" checked>\n        <input type=\"checkbox\" name=\"showIcon\" title=\"Icon\" checked>\n        <input type=\"color\" name=\"icon\" title=\"Icon Color\" value=\"#078efb\" condition=\"showIcon\">\n        <input type=\"color\" name=\"card\" title=\"Card BG\" value=\"#121212\">\n        <input type=\"color\" name=\"border\" title=\"Card Border\" value=\"#252525\">\n        <input type=\"color\" name=\"active\" title=\"Active Color\" value=\"#078efb\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax required>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/planet-hero.gif\">\n            <input type=\"color\" title=\"Color\" value=\"#060606\">\n            <input type=\"video\" title=\"Video\" value=\"https://youtu.be/nfDTQ6FdSp4\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/starbackground-2000x1333.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#060606\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"content-wrapper\">\n                    <div class=\"logo-wrapper\">\n                        <img mbr-if=\"showLogo\" src=\"@PROJECT_PATH@/assets/images/connections-236x153.png\" alt=\"Logo\">\n                    </div>\n                    <h1 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .logo-wrapper, .mbr-section-btn\">Work, Share, Create</h1>\n                    <div class=\"text-wrapper\" mbr-if=\"showText\">\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-text, .text-wrapper\">\n                            An&nbsp;<span>entirely exclusive</span>&nbsp;online platform for students to <span>share work and be productive</span>.\n                        </p>\n                    </div>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-secondary\" href=\"/start\" data-app-placeholder=\"Type Text\">\n                            <span class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont mbr-iconfont-btn\"></span>\n                            Get Started</a></div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
